# Haven - Environment Variables
# Copy this file to .env and fill in your values

# ElevenLabs API Key (required for voice conversations)
# Get your key from: https://elevenlabs.io/app/settings/api-keys
EXPO_PUBLIC_ELEVENLABS_API_KEY=sk_your_elevenlabs_api_key_here

# Supabase Configuration (for user authentication)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API URL (optional - defaults to Vercel deployment)
EXPO_PUBLIC_API_URL=https://spectrum-connect-start.vercel.app
